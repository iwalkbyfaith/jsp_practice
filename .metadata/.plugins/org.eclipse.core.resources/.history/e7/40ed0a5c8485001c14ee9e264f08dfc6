	저장된 쿠키 가져오기



 -->


<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    
    
<%
	// 1. 저장된 쿠키 가져오기
	Cookie[] cookies = request.getCookies();

	String str = null;
	
	boolean flag = false;
	
	for(int i=0; i< cookies.length; i++){
		// 쿠키 이름을 얻어오는 메서드
		str = cookies[i].getName();
		System.out.println(str);
	}


%>
<!DOCTYPE html>
<html>
<head>
